//Write a program that gets n as input and print all the prime numbers till that number.
#include <iostream>
#include <math.h>
using namespace std;
int main() {
    int n=11;
    for(int x=2;x<=n;x++){
        int flag = 0;
        for(int i=2;i<x;i++){
            if(x%i==0){
                flag = 1;
            }
        }
        if(flag==0){
            cout<<x << endl;
        }
    }
    return 0;
}
